import java.util.Scanner;
public class primeno {
	public static void main(String args[])
	{
		int i=2 ,n;
		Scanner sc = new Scanner(System.in);
		System.out.println("enter the number");
		n= sc.nextInt();
		
//		for(i=2;i<=n-1;i++)
	while(i<=n-1)
		{
			if(n%i==0)
			{
			System.out.println("not a prime no");
				break;
			}
			i++;
			
			
	}

}
}
